#---------------------------------------------------------------------------  
# Project  
#---------------------------------------------------------------------------  
PROJECT_NAME           = "MatPlotOpenCV"
PROJECT_NUMBER         = @PROJECT_VERSION@
OUTPUT_DIRECTORY       = @CMAKE_BINARY_DIR@/docs
CREATE_SUBDIRS         = NO

#---------------------------------------------------------------------------  
# Build  
#---------------------------------------------------------------------------  
EXTRACT_ALL            = NO
EXTRACT_PRIVATE        = NO
EXTRACT_STATIC         = NO
EXTRACT_LOCAL_CLASSES  = NO
OPTIMIZE_OUTPUT_FOR_C  = NO
OPTIMIZE_OUTPUT_JAVA   = NO

#---------------------------------------------------------------------------  
# Input  
#---------------------------------------------------------------------------  
INPUT                  = MatPlotOpenCV/include/
FILE_PATTERNS          = *.h *.hpp
RECURSIVE              = YES

#---------------------------------------------------------------------------  
# Diagrams  
#---------------------------------------------------------------------------  
HAVE_DOT               = YES
DOT_IMAGE_FORMAT       = svg

#---------------------------------------------------------------------------  
# Output formats  
#---------------------------------------------------------------------------  
GENERATE_HTML          = YES
GENERATE_LATEX         = NO
